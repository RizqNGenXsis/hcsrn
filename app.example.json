{
  "expo": {
    "name": "Rizq.Earth",
    "slug": "rizq_earth",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "0.7.3",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "icon": "./assets/images/ios.icon.png",
      "bundleIdentifier": "rizq.earth",
      "supportsTablet": true,
      "buildNumber": "31"
    },
    "android": {
      "icon": "./assets/images/android.icon.png",
      "useNextNotificationsApi": false,
      "package": "rizq.earth",
      "versionCode": 31
    },
    "userInterfaceStyle": "automatic",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "rizq",
            "project": "rizq.earth",
            "authToken": null
          }
        }
      ]
    }
  }
}
