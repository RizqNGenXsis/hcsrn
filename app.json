    "android": {
      "icon": "./assets/images/android.icon.png",
      "useNextNotificationsApi": true,
      "package": "rizq.invoice",
      "versionCode": 31
    },
    "userInterfaceStyle": "automatic",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "rizqinvo",
            "project": "rizq.invoice",
            "authToken": null
          }
        }
      ]
    },
    "description": "Pay as you Go! Cloud Services Billings Inventories & Recurring Invoicing xSaaS.\n",
